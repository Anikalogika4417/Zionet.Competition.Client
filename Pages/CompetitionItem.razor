@using ClientEntity
@inject DataService data
@inject HttpClient Http

<div>
    <h1>Competition Name</h1> @curentComp.competition_name
    <br>
    <h1>Competition start</h1> @curentComp.competition_start
    <br>
    <h1>Competition end</h1> @curentComp.competition_end
    <br>
    <h1>Task quantity</h1> @curentComp.task_quantity
    <br>

    <div class="nav-item px-3">
        <NavLink class="nav-link" href="@($"/Competition/{@curentComp.id}/tasks")">
            <span class="oi oi-list-rich" aria-hidden="true"></span> Tasks
        </NavLink>
    </div>

    <div class="nav-item px-3">
        <NavLink class="nav-link" href="@($"/Competition/{@curentComp.id}/categories")">
            <span class="oi oi-list-rich" aria-hidden="true"></span> Categories
        </NavLink>
    </div>

    <div class="nav-item px-3">
        <NavLink class="nav-link" href="@($"/Competition/{@curentComp.id}/groups")">
            <span class="oi oi-list-rich" aria-hidden="true"></span> Groups
        </NavLink>
    </div>

    @* <div class="nav-item px-3">
        <NavLink class="nav-link" href="CompetitionList">
            <span class="oi oi-list-rich" aria-hidden="true"></span> Competitions
        </NavLink>
    </div> *@
</div>

@code{
    [Parameter]
    public MCompetition curentComp {get; set;}
}